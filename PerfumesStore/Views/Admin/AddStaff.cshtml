@model PerfumesStore.Models.RegistrationViewModel

<div class="container shadow rounded-3 p-3">
	<h2>Add member</h2><hr />

	<form class="form" method="post" asp-action="AddStaff">
		<div class="row m-3">
			<label class="col-form-label col-sm-2 col-12" asp-for="Username"></label>
			<div class="col">
				<input class="form-control" type="text" asp-for="Username" />
			</div>
		</div>
		<div class="row m-3">
			<label class="col-form-label col-sm-2 col-12" asp-for="Email"></label>
			<div class="col">
				<input class="form-control" type="text" asp-for="Email" />
			</div>
		</div>
		<div class="row m-3">
			<label class="col-form-label col-sm-2 col-12" asp-for="Address"></label>
			<div class="col">
				<input class="form-control" type="text" asp-for="Address" />
			</div>
		</div>
		<div class="row m-3">
			<label class="col-form-label col-sm-2 col-12" asp-for="Password"></label>
			<div class="col">
				<input class="form-control" type="password" asp-for="Password" />
			</div>
		</div>
		<div class="d-grid">
			<input class="btn btn-light shadow-sm" type="submit" value="submit" />
		</div>

		<div asp-validation-summary="All"></div>
	</form>
</div>

<script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.5.1.min.js"></script>
<script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.17.0/jquery.validate.min.js"></script>
<script src="https://ajax.aspnetcdn.com/ajax/jquery.validation.unobtrusive/3.2.10/jquery.validate.unobtrusive.min.js"></script>


